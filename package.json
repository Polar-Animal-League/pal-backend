{
  "name": "@pal/pal-backend",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "dev:backend": "tsc && cross-conf-env docker-compose up --build app",
    "dev:deps": "cross-conf-env docker-compose up db",
    "dev:watch": "concurrently --kill-others -n Deps,Backend -c green,blue \"npm run dev:deps\" \"nodemon --watch src -e ts,json --exec \\\"npm run dev:backend\\\"\""
  },
  "author": "Micha_i",
  "repository": {
    "type": "git",
    "url": "git://github.com/Polar-Animal-League/pal-backend.git"
  },
  "license": "",
  "dependencies": {
    "@polar-animal-league/shared": "0.0.1",
    "colors": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.13",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "concurrently": "^5.3.0",
    "cross-conf-env": "^1.2.1",
    "eslint": "^7.15.0",
    "eslint-formatter-github": "^1.0.11",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.3"
  }
}
